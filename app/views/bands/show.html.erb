<div>
  <div class="d-flex justify-content-between bg-white">
    <div class="m-3">
      <h4>All your songs</h4>
    </div>
    <div class="d-flex justify-content-between">
      <div class="mt-3 mr-3">
        <form method="get" action="/bands/new">
          <div><input value="New song" type="submit" class="btn btn-primary" /></div>
        </form>
      </div>
      <div class="mt-3 mr-3">
        <form method="get" action="/bands/new">
          <div><input value="Settings" type="submit" class="btn btn-primary" /></div>
        </form>
      </div>
    </div>
  </div>
  <div class="song-card-container">
    <% @songs.each do |song| %>
    <div class="card-song">
      <div class="card-product-infos">
        <h2><%= song.name %></h2>
        <p><%= song.description %></p>
        <%= link_to 'PARTITIONS', band_song_path(song.band_id, song.id), class: "btn btn-primary" %>
      </div>
    </div>
    <% end %>
  </div>
</div>
