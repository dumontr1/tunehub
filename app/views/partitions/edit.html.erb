<div>
  <div class="d-flex justify-content-between bg-white p-sticky-top border-bottom">
    <div class="my-auto pl-5 pages-title">
      <h4>Edit partition</h4>
    </div>
    <div class="d-flex justify-content-between">
      <div class="m-2 pr-3">
        <%= link_to "Back", :back, class: "btn btn-primary" %>
      </div>
    </div>
    <%# LE HTML C'EST SUPER %>
  </div>
    <div id="embed-container"></div>
    <script src="https://prod.flat-cdn.com/embed-js/v1.3.0/embed.min.js"></script>
    <%= javascript_tag do %>
      var container = document.getElementById("embed-container");
      var embed = new Flat.Embed(container, {
        "width": "100%",
        "height": "800",
        "score": "<%= @partition.score %>",
        "embedParams": {
          "mode": "edit",
          "appId": "<%= @partition.appid %>"
        },
      });
    <% end %>
</div>
